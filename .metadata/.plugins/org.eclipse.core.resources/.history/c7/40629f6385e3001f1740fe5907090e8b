package com.petfoster.model;

import java.time.LocalDate;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;

@Entity
public class PetHistory {
	
	/*
	 * pet_id, foster_start_date, foster_end_date, shelter_id, foster_parent_id, medical_history, notes
	 */
	@Id
	private Long historyId;
	private Long petId;
	private Long adopterUserId;
	private Long fosterParentId;
	private LocalDate fosterStartDate;
	private LocalDate fosterEndDate;
	private Long shelterId;
	private Long medicalRecordId;
	

}
